
<div class="row">
    <div class="col s12 grey">
        <img class="burger" src="/assets/img/Burger.png" alt="A Burger" height="236" width="319">
        <h1 class="center">Eat-Da-Burger</h1>
    </div>

    <div class="col s6 blue">
        <ul>
            {{#each burgers}}
                {{#unless devoured}}
                    {{> burgers/burger-block devoured=true}}
                {{/unless}}
            {{/each}}
        </ul>
    </div>


    <div class="col s6 red">
        <ul>
            {{#each burgers}}
                {{#unless devoured}}
                    {{> burgers/burger-block devoured=false}}
                {{/unless}}
            {{/each}}
        </ul>
    </div>
</div>






